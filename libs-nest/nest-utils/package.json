{
  "name": "@lskjs/nest-utils",
  "version": "3.8.0",
  "description": "LSK.js – nest utils – helpers and utils for Nest.js projects",
  "author": "Igor Suvorov <hi@isuvorov.com> (https://github.com/isuvorov)",
  "private": false,
  "scripts": {
    "dev": "            lsk run dev",
    "build": "          lsk run build",
    "test": "           lsk run test",
    "prepack": "        lsk run prepack",
    "release": "        lsk run release"
  },
  "dependencies": {
    "@lskjs/algos": "^3.3.0",
    "@lskjs/env": "^3.8.0",
    "@lskjs/err": "^3.3.0",
    "@lskjs/log": "^3.8.0",
    "@lskjs/webserver": "^3.1.1",
    "@types/express": "^4.17.17",
    "fast-safe-stringify": "2.1.1",
    "nanoid": "^3.1.29"
  },
  "devDependencies": {
    "@nestjs/common": "^10.1.2",
    "@nestjs/config": "^3.0.0",
    "express": "4.18.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1"
  },
  "//": "///////////========================/////////========================/////////========================/////////",
  "prettier": "@lskjs/eslint-config/prettier",
  "eslintConfig": {
    "extends": "@lskjs/eslint-config"
  },
  "jest": {
    "preset": "@lskjs/jest-config"
  },
  "files": [
    "lib",
    "README.md",
    "LICENCE"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js"
    }
  },
  "size-limit": [
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AccessLoggerMiddleware.js",
      "limit": "10kb"
    },
    {
      "path": "lib/AnyExceptionFilter.js",
      "limit": "10kb"
    },
    {
      "path": "lib/createNestLogger.js",
      "limit": "10kb"
    },
    {
      "path": "lib/Time.decorator.js",
      "limit": "10kb"
    }
  ],
  "///": "//////////========================/////////========================/////////========================/////////",
  "repository": "https://github.com/lskjs/lskjs/tree/master//libs-nest/nest-utils",
  "homepage": "https://github.com/lskjs/lskjs/tree/master//libs/nest-utils",
  "bugs": "https://github.com/lskjs/lskjs/issues",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "////": "/////////========================/////////========================/////////========================/////////"
}
