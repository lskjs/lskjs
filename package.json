{
  "version": "0.1.3",
  "description": "Lego starter kit command line helper",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/isuvorov/lib-starter-kit",
  "keywords": [],
  "scripts": {
    "commit": "./node_modules/cz-customizable/standalone.js -S",
    "dev": "./scripts/dev.sh",
    "release": "lerna publish --skip-npm",
    "release:patch": "lerna publish --skip-npm  -m \"release: %s\" --yes patch",
    "release:it": "release-it",
    "release:changelog": "lerna-changelog",
    "build-locales": "./scripts/build-locales.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "update-starter-kit": "./scripts/check-unstaged-files.sh && ./scripts/update-starter-kit.sh",
    "update": "lerna exec -- npm run update",
    "update-force": "lerna exec -- ../../scripts/package-update.sh",
    "build": "./scripts/build.sh",
    "watch": "lerna run --parallel watch",
    "link": "lerna exec -- npm run link",
    "bump": "lerna exec -- npm run bump",
    "storybook": "./scripts/storybook.sh",
    "pre-commit": "npm run lint:staged",
    "pre-push": "npm run lint:prettier",
    "prettier": "prettier",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:spellcheck",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:prettier": "prettier --list-different \"./**/*.js\"",
    "lint:js": "lerna exec -- npm run lint:js",
    "lint:md": "remark -q .",
    "lint:spellcheck": "./scripts/spellcheck.sh",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "jest": "jest --coverage --verbose --color",
    "test": "npm run lint && npm run jest",
    "pretty-staged": "pretty-quick --staged"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@lskjs/linkall": "^0.37.2",
    "@lskjs/log": "^0.37.4",
    "@release-it/conventional-changelog": "1.1.0",
    "@storybook/react": "^5.3.12",
    "@types/react": "^16.8.13",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-transform-class-property-arrow-to-bind": "^1.1.0",
    "babel-preset-minify": "^0.5.0",
    "commitizen": "^4.0.3",
    "company-commit": "^1.1.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "cz-customizable": "6.2.0",
    "dnsjack": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.12.4",
    "glob": "^7.1.6",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lerna": "^3.13.1",
    "lerna-changelog": "^1.0.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "npm-check-updates": "^4.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.8.6",
    "react-styleguidist": "^10.6.1",
    "react-test-renderer": "^16.8.6",
    "release-it": "12.4.3",
    "release-it-lerna-changelog": "^1.0.3",
    "remark": "^11.0.2",
    "remark-cli": "^7.0.1",
    "remark-preset-lint-recommended": "^3.0.2",
    "shelljs": "^0.8.3",
    "size-limit": "^4.0.0",
    "storybox": "^1.1.0",
    "typescript": "^3.7.5",
    "webpack": "^4.29.6",
    "yaspeller": "^6.0.2"
  },
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
