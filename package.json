{
  "version": "0.1.3",
  "description": "Lego starter kit command line helper",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/lskjs/lskjs",
  "keywords": [],
  "scripts": {
    "commit": "./node_modules/cz-customizable/standalone.js -S",
    "dev": "./scripts/dev.sh",
    "release": "lerna publish --skip-npm",
    "release:patch": "lerna publish --skip-npm  -m \"release: %s\" --yes patch",
    "release:it": "release-it",
    "release:changelog": "lerna-changelog",
    "build-locales": "./scripts/build-locales.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "update-starter-kit": "./scripts/check-unstaged-files.sh && ./scripts/update-starter-kit.sh",
    "update": "lerna exec -- npm run update",
    "update-force": "lerna exec -- ../../scripts/package-update.sh",
    "build": "./scripts/build.sh",
    "watch": "lerna run --parallel watch",
    "link": "lerna exec -- npm run link",
    "bump": "lerna exec -- npm run bump",
    "storybook": "./scripts/storybook.sh",
    "pre-commit": "npm run lint:staged",
    "pre-push": "npm run lint:prettier",
    "prettier": "prettier",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:spellcheck",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:prettier": "prettier --list-different \"./**/*.js\"",
    "lint:js": "lerna exec -- npm run lint:js",
    "lint:md": "remark -q .",
    "lint:spellcheck": "./scripts/spellcheck.sh",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "jest": "jest --coverage --verbose --color",
    "test": "npm run lint && npm run jest",
    "pretty-staged": "pretty-quick --staged"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "7.8.3",
    "@babel/plugin-proposal-function-bind": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.4",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-stage-0": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@babel/runtime": "7.8.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/prompt": "8.3.5",
    "@lskjs/linkall": "1.0.14",
    "@lskjs/log": "1.0.14",
    "@release-it/conventional-changelog": "1.1.0",
    "@storybook/react": "5.3.13",
    "@types/react": "16.9.21",
    "@types/storybook__react": "5.2.1",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "babel-plugin-emotion": "10.0.27",
    "babel-plugin-root-import": "6.4.1",
    "babel-plugin-transform-class-property-arrow-to-bind": "1.1.0",
    "babel-preset-minify": "0.5.1",
    "commitizen": "4.0.3",
    "company-commit": "1.1.1",
    "concurrently": "5.1.0",
    "cross-env": "7.0.0",
    "cz-conventional-changelog": "3.1.0",
    "cz-customizable": "6.2.0",
    "dnsjack": "1.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "glob": "7.1.6",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lerna": "3.20.2",
    "lerna-changelog": "1.0.0",
    "lint-staged": "10.0.7",
    "nodemon": "2.0.2",
    "npm-check-updates": "4.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "react": "16.8.6",
    "react-styleguidist": "10.6.2",
    "react-test-renderer": "16.8.6",
    "release-it": "12.6.0",
    "release-it-lerna-changelog": "1.0.3",
    "remark": "11.0.2",
    "remark-cli": "7.0.1",
    "remark-preset-lint-recommended": "3.0.3",
    "shelljs": "0.8.3",
    "size-limit": "4.0.2",
    "storybox": "1.1.0",
    "typescript": "3.8.2",
    "webpack": "4.41.6",
    "yaspeller": "6.0.2"
  },
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
