{
  "version": "2.0.0",
  "description": "Lego starter kit command line helper",
  "author": "Igor Suvorov <hi@isuvorov.com>",
  "license": "MIT",
  "repository": "https://github.com/isuvorov/lib-starter-kit",
  "keywords": [],
  "scripts": {
    "commit": "./node_modules/cz-customizable/standalone.js -S",
    "start": "lerna exec -- npm run start",
    "dev": "./scripts/dev.sh",
    "dev:cra-and-server": "lerna exec -- npm run dev:cra-and-server",
    "dev:cra": "lerna exec -- npm run dev:cra",
    "dev:server": "lerna exec -- npm run dev:server",
    "release": "lerna publish --skip-npm --exact && npm run release:after",
    "release:yes": "lerna publish --skip-npm --exact --github-release --yes && npm run release:after",
    "release:after": "./scripts/release-after.sh",
    "release:it": "release-it",
    "release:changelog": "lerna-changelog",
    "release:storybook": "storybook-to-ghpages",
    "build-locales": "./scripts/build-locales.sh",
    "i18": "./scripts/i18.sh",
    "bootstrap": "./scripts/bootstrap.sh",
    "dns": "./scripts/dns.sh",
    "linkwatch": "./scripts/linkwatch.sh",
    "clear": "./scripts/clear.sh",
    "update-starter-kit": "./scripts/check-unstaged-files.sh && ./scripts/update-starter-kit.sh",
    "update": "lerna exec -- npm run update",
    "update-force": "lerna exec -- ../../scripts/package-update.sh",
    "audit": "lerna exec --parallel --no-bail -- npm audit",
    "build": "./scripts/build.sh",
    "watch": "lerna run --parallel watch",
    "link": "lerna exec -- npm run link",
    "bump": "lerna exec -- npm run bump",
    "storybook": "./scripts/storybook.sh",
    "pre-commit": "echo npm run lint:staged",
    "pre-push": "echo npm run lint:prettier",
    "prettier": "prettier",
    "lint": "npm run lint:js && npm run lint:md && npm run lint:spellcheck",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint . --ext .js --fix && prettier --write --plugin-search-dir=.",
    "lint:prettier": "prettier --list-different \"./**/*.js\"",
    "lint:js": "lerna exec -- npm run lint:js",
    "lint:md": "remark -q .",
    "lint:spellcheck": "./scripts/spellcheck.sh",
    "size": "size-limit",
    "size:why": "size-limit --why",
    "test:coverage": "jest --coverage --verbose --color",
    "test:jest": "jest --verbose --color",
    "test": "npm run lint && npm run test:jest-coverage",
    "pretty-staged": "pretty-quick --staged",
    "contributors:add": "node_modules/.bin/all-contributors add",
    "contributors:generate": "node_modules/.bin/all-contributors generate"
  },
  "dependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-proposal-decorators": "7.12.12",
    "@babel/plugin-proposal-export-namespace-from": "7.12.1",
    "@babel/plugin-proposal-function-bind": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-modules-commonjs": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@babel/preset-stage-0": "7.8.3",
    "@babel/preset-typescript": "7.12.7",
    "@babel/runtime": "7.12.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@commitlint/prompt": "11.0.0",
    "@emotion/babel-plugin": "^11.1.2",
    "@lskjs/build-locales": "1.98.0",
    "@lskjs/linkall": "1.89.2",
    "@lskjs/log": "1.98.0",
    "@release-it/conventional-changelog": "2.0.0",
    "@storybook/react": "6.1.15",
    "@storybook/storybook-deployer": "2.8.7",
    "@types/config": "0.0.38",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.0",
    "@types/storybook__react": "5.2.1",
    "@typescript-eslint/eslint-plugin": "4.14.0",
    "@typescript-eslint/parser": "4.14.0",
    "all-contributors-cli": "6.19.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-transform-class-property-arrow-to-bind": "1.1.0",
    "babel-preset-minify": "0.5.1",
    "commitizen": "4.2.3",
    "company-commit": "1.1.1",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "cz-customizable": "6.3.0",
    "dnsjack": "1.0.0",
    "eslint": "7.18.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "glob": "7.1.6",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lerna-changelog": "1.0.1",
    "lint-staged": "10.5.3",
    "nodemon": "2.0.7",
    "npm-check-updates": "11.0.2",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "17.0.1",
    "react-scripts": "4.0.1",
    "react-test-renderer": "17.0.1",
    "release-it": "14.2.2",
    "release-it-lerna-changelog": "3.1.0",
    "remark": "13.0.0",
    "remark-cli": "9.0.0",
    "remark-preset-lint-recommended": "5.0.0",
    "shelljs": "0.8.4",
    "size-limit": "4.9.1",
    "storybox": "1.1.0",
    "ts-interface-builder": "0.2.3",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3",
    "webpack": "5.17.0",
    "webpack-dev-server": "3.11.2",
    "yaspeller": "7.0.0"
  },
  "config": {
    "cz-customizable": {
      "config": ".czrc.js"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
